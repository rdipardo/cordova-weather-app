{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "layoutFile": "layout.tmpl",
            "staticFiles": {
                "include": [
                  ".github/jsdoc"
                ],
                "excludePattern": ".+\\.(tmpl|js(on|x|))$"
            }
        }
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "doc/",
        "recurse": true,
        "readme": "README.md"
    },
    "project": {
        "version": "1.2.0",
        "repo": "https://github.com/rdipardo/cordova-weather-app/"
    }
}
